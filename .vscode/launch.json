    "version": "0.2.0",
    "configurations": [
        {
            "name": "openocd",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/prog.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "svdFile": "./lib/GD32VF103.svd", 
            "searchDir": ["/usr/share/openocd/scripts"],           
